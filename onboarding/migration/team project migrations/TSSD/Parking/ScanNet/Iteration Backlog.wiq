<?xml version="1.0" encoding="utf-8"?>
<WorkItemQuery Version="1">
	<TeamFoundationServer>http://tfs13.archon-tech.com:8080/tfs/parking</TeamFoundationServer>
	<TeamProject>ScanNet</TeamProject>
	<Wiql>SELECT [System.Id], [System.WorkItemType], [System.Title], [System.State], [System.AssignedTo], [Microsoft.VSTS.Scheduling.RemainingWork], [Microsoft.VSTS.Scheduling.CompletedWork], [Microsoft.VSTS.Scheduling.StoryPoints], [Microsoft.VSTS.Common.StackRank], [Microsoft.VSTS.Common.Priority], [Microsoft.VSTS.Common.Activity], [System.IterationPath], [System.AreaPath] FROM WorkItemLinks WHERE Source.[System.TeamProject] = @project and Source.[System.WorkItemType] in group 'Microsoft.RequirementCategory' and Source.[System.IterationPath] under @project and Source.[System.State] in ('New', 'Active', 'Resolved', 'Closed') and Source.[System.AreaPath] under @project and [System.Links.LinkType] = 'System.LinkTypes.Hierarchy-Forward' and ((Target.[System.WorkItemType] in group 'Microsoft.TaskCategory' and Target.[System.IterationPath] under 'ScanNet\Iteration 1' and Target.[System.State] in ('New', 'Active', 'Closed') and Target.[System.AreaPath] under @project) or (Target.[System.WorkItemType] in group 'Microsoft.RequirementCategory' and Target.[System.IterationPath] under 'ScanNet\Iteration 1' and Target.[System.State] in ('New', 'Active', 'Resolved', 'Closed') and Target.[System.AreaPath] under @project)) ORDER BY [Microsoft.VSTS.Common.StackRank], [System.Id] mode(Recursive,ReturnMatchingChildren)</Wiql>
</WorkItemQuery>