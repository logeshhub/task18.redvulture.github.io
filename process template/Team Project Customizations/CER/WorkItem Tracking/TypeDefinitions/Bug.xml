      </FIELD>
      <FIELD name="DetectedInIterationPath" refname="Microsoft.VSTS.Common.DetectedInIterationPath" type="String" reportable="dimension" />
      <FIELD name="CustomerSite" refname="Microsoft.VSTS.Common.CustomerSite" type="String" />
      <FIELD name="CustomerState" refname="Microsoft.VSTS.Common.CustomerState" type="String">
