          <LISTITEM value="All [Core]" />
          <LISTITEM value="All [Core] ICD-10-AM" />
          <LISTITEM value="All [Core] Intl" />
          <LISTITEM value="Australia" />
          <LISTITEM value="Belgium" />
          <LISTITEM value="Brazil" />
          <LISTITEM value="Canada" />
          <LISTITEM value="Chile" />
          <LISTITEM value="Columbia" />
          <LISTITEM value="Germany" />
          <LISTITEM value="Hong Kong" />
          <LISTITEM value="Indonesia" />
          <LISTITEM value="Ireland" />
          <LISTITEM value="Italy" />
          <LISTITEM value="Lithuania" />
          <LISTITEM value="Malaysia" />
          <LISTITEM value="Mexico" />
          <LISTITEM value="New Zealand" />
          <LISTITEM value="Portugal" />
          <LISTITEM value="Portugal MOH" />
          <LISTITEM value="Puerto Rico" />
          <LISTITEM value="Qatar" />
          <LISTITEM value="Researchers Kit" />
          <LISTITEM value="Saudi Arabia" />
          <LISTITEM value="Singapore" />
          <LISTITEM value="South Africa" />
          <LISTITEM value="Spain" />
          <LISTITEM value="Switzerland" />
