          <ALLOWEDVALUES expanditems="true">
            <LISTITEM value="ACT" />
            <LISTITEM value="NSW" />
            <LISTITEM value="NT" />
            <LISTITEM value="NZ" />
            <LISTITEM value="QLD" />
            <LISTITEM value="SA" />
            <LISTITEM value="TAS" />
            <LISTITEM value="VIC" />
            <LISTITEM value="WA" />
          </ALLOWEDVALUES>
        </WHEN>
        <WHEN field="Microsoft.VSTS.Common.Country" value="Canada">
